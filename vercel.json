{
  "version": 2,
  "name": "imanix-b2b-portal",
  "builds": [
    {
      "src": "server-auth.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server-auth.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server-auth.js": {
      "maxDuration": 30
    }
  }
} 